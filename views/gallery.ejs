<ol class="carousel-indicators">
  <% gallery.forEach(function(photo, index) { %>
  <li 
    data-target="#carousel-<%= index %>" 
    data-slide-to="<%= index %>">
  </li>
<% }); %>
</ol>
<div class="carousel-inner">
  <% gallery.forEach(function(photo, index) { %>
  <div class="carousel-item">
    <img src="../images/<%= photo.photo %>" alt="slide" />
  </div>
  <% }); %>
</div>





